{
  "expo": {
    "name": "SupDoc",
    "slug": "supdoc",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "supdoc",
    "privacy": "public",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#020912"
    },
    "locales": {
      "en": "./assets/locales/en.json",
      "es": "./assets/locales/es.json",
      "zh": "./assets/locales/zh.json",
      "hi": "./assets/locales/hi.json",
      "pt": "./assets/locales/pt.json",
      "ar": "./assets/locales/ar.json",
      "fr": "./assets/locales/fr.json",
      "de": "./assets/locales/de.json",
      "ru": "./assets/locales/ru.json",
      "ja": "./assets/locales/ja.json",
      "ko": "./assets/locales/ko.json",
      "it": "./assets/locales/it.json"
    },    
    "ios": {
      "supportsTablet": false,
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "CFBundleLocalizations": ["en", "es", "zh", "hi", "pt", "ar", "fr", "de", "ru", "ja", "ko", "it"],
        "CFBundleDevelopmentRegion": "es"
      },
      "bundleIdentifier": "com.notaroomba.supdoc"
    },
    "android": {
      "package": "com.notaroomba.supdoc",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#020912"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a6219396-5a11-49f4-8118-8d2692a4d80b"
      }
    },
    "owner": "notaroomba"
  }
}
